<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qy.mapper.PropertyResourceMapper">

   <select id="queryPropertyValueBypropertyName" resultType="java.lang.String" parameterType="java.lang.String">
    
     SELECT property_value FROM t_sys_property_resource WHERE property_name = #{propertyName}

   </select>

</mapper>